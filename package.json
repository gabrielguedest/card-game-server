{
  "name": "card-game-server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.14.141",
    "@types/node": "^12.7.8",
    "@types/socket.io": "^2.1.3",
    "@types/uuid": "^3.4.5",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "nodemon": "^1.19.2",
    "socket.io": "^2.3.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3",
    "uuid": "^3.3.3"
  }
}
